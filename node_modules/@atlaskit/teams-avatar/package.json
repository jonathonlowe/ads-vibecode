			],
			"imports": [
				"import-no-extraneous-disable-for-examples-and-docs"
			]
		}
	},
	"platform-feature-flags": {
		"enable-team-avatar-switch": {
			"type": "boolean"
		},
		"team-avatar-radii": {
			"type": "boolean"
		}
