{
	"name": "@atlaskit/teams-avatar",
	"version": "2.3.2",
	"description": "A team avatar is a visual representation of a team.",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
