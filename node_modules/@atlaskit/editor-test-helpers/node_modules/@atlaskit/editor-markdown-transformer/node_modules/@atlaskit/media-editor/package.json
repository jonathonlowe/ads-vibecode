{
  "name": "@atlaskit/media-editor",
  "version": "37.0.12",
  "description": "Media component for image annotations",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend",
  "author": "Atlassian Pty Ltd",
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/cjs/index.d.ts",
  "sideEffects": false,
  "atlaskit:src": "src/index.tsx",
  "atlassian": {
    "team": "Media Platform",
    "inPublicMirror": true,
    "releaseModel": "scheduled",
    "website": {
      "name": "Media Editor"
    }
  },
  "scripts": {
    "check-dist": "ls -d dist/*/engine | xargs -I{} test -d {}/core/binaries",
    "copy-binaries": "ls -d dist/*/engine | xargs -n 1 -I{} cp -r src/engine/core/binaries {}/core",
    "ak-postbuild": "yarn copy-binaries && yarn check-dist"
  },
  "dependencies": {
    "@atlaskit/analytics-next": "^6.3.5",
    "@atlaskit/button": "^13.4.0",
    "@atlaskit/field-range": "^8.0.2",
    "@atlaskit/icon": "^20.1.0",
    "@atlaskit/inline-dialog": "^12.1.11",
    "@atlaskit/media-card": "^68.0.0",
    "@atlaskit/media-client": "^7.0.0",
    "@atlaskit/media-ui": "^12.5.0",
    "@atlaskit/modal-dialog": "^10.6.0",
    "@atlaskit/spinner": "^14.0.0",
    "@atlaskit/theme": "^9.5.1",
    "@atlaskit/tooltip": "^15.2.5",
    "@types/uuid": "^3.4.4",
    "perf-marks": "^1.5.0",
    "tslib": "^1.9.3",
    "uuid": "^3.1.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-intl": "^2.6.0",
    "rxjs": "^5.5.0",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@atlaskit/build-utils": "*",
    "@atlaskit/docs": "^8.5.0",
    "@atlaskit/media-test-helpers": "^27.1.0",
    "@atlaskit/ssr": "*",
    "enzyme": "^3.10.0",
    "react-dom": "^16.8.0",
    "react-intl": "^2.6.0",
    "rxjs": "^5.5.0",
    "wait-for-expect": "^1.2.0"
  }
}
