{
  "name": "@loomhq/record-sdk",
  "description": "The Loom Software Development Kit",
  "version": "2.36.18",
  "author": "@loomhq/platform",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.18.10",
    "@emotion/cache": "^11.10.1",
    "@emotion/core": "^10.1.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^10.0.27",
    "@loomhq/enums": "^2.3.5",
    "@loomhq/esbuild-plugin-umd": "^0.4.2",
    "@loomhq/lens": "^10.83.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@types/react-redux": "^7.1.24",
    "@types/redux-mock-store": "^1.0.2",
    "@types/ua-parser-js": "^0.7.36",
    "babel-jest": "^26.6.3",
    "cypress": "^9.1.1",
    "date-fns": "^2.29.3",
    "deepmerge": "^4.2.2",
    "esbuild-plugin-text-replace": "^1.1.3",
    "eventemitter3": "^4.0.7",
    "jest": "^27.5.1",
    "jest-junit": "^15.0.0",
    "jwt-decode": "^3.1.2",
    "memoize-one": "^5.1.1",
    "prop-types": "^15.8.1",
    "raf": "^3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.3",
    "react-error-boundary": "^3.1.0",
    "react-redux": "^7.2.6",
    "react-shadow": "^19.0.3",
    "redux": "^4.2.0",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "rollup": "^2.57.0",
    "rollup-plugin-dts": "^4.0.0",
    "serialize-error": "^8.0.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.9.4",
    "ua-parser-js": "^0.7.25",
    "@loomhq/bundler": "1.0.14",
    "@loomhq/iframe-redux": "0.1.12",
    "@loomhq/camera-bubble": "12.4.18",
    "@loomhq/outcomes": "2.1.43",
    "@loomhq/media-utilities": "2.1.10",
    "@loomhq/sdk-background": "1.26.44",
    "@loomhq/third-party-cookie-check": "2.0.14",
    "@loomhq/sdk-store": "0.23.16",
    "@loomhq/tsconfig": "1.0.5"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json",
    "./is-supported": {
      "require": "./dist/cjs/is-supported.js",
      "import": "./dist/esm/is-supported.js"
    }
  },
  "files": [
    "/dist/esm",
    "/dist/cjs",
    "/dist/record-sdk.d.ts",
    "/dist/is-supported.d.ts"
  ],
  "jest-junit": {
    "suiteName": "@loomhq/record-sdk tests"
  },
  "license": "SEE LICENSE IN license.md",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/record-sdk.d.ts",
  "typesVersions": {
    "*": {
      "is-supported": [
        "dist/is-supported.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "node build.mjs",
    "build:types": "pnpm build:types:declarations && pnpm build:types:bundle",
    "build:types:bundle": "rollup -c || true",
    "build:types:declarations": "tsc",
    "ci/lint": "pnpm lint --quiet --cache --cache-location /tmp/eslintcache/ --cache-strategy content",
    "ci/test:unit": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "clean:build": "rm -rf ./dist && rm -f tsconfig.tsbuildinfo",
    "lint": "eslint \"**/*.{js,ts,jsx,tsx,graphql}\"",
    "test": "jest --maxWorkers=2",
    "test:cypress": "cypress run"
  }
}